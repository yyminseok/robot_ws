#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'm-pubsub','console_scripts','mp'
__requires__ = 'm-pubsub'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('m-pubsub', 'console_scripts', 'mp')()
    )
